{
  "name": "quick-credit",
  "version": "1.0.0",
  "description": "A simple Loan App",
  "main": "app.js",
  "scripts": {
    "test": "nyc mocha --exit --timeout 60000 --require babel-register",
    "testDev": "SET NODE_ENV=test&&nyc mocha --exit --timeout 40000 --require babel-register",
    "build": "babel server -d dist",
    "start": "npm run build && node ./dist/app",
    "startDev": "SET NODE_ENV=development&& npm run build && node ./dist/app",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/successgilli/quick-credit.git"
  },
  "keywords": [
    "loan",
    "andela"
  ],
  "author": "Gilbert Awaji-mitop NchereAwaji",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/successgilli/quick-credit/issues"
  },
  "homepage": "https://github.com/successgilli/quick-credit#readme",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "cloudinary": "1.14.0",
    "cors": "2.8.5",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "jsonwebtoken": "8.5.1",
    "mocha": "6.1.4",
    "multer": "1.4.1",
    "multer-storage-cloudinary": "2.2.1",
    "nodemailer": "6.1.1",
    "pg": "7.11.0",
    "swagger-ui-express": "4.0.2",
    "typescript": "3.4.5",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "nyc": "14.0.0"
  }
}
